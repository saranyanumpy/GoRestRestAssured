{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["meta", "data"],
  "properties": {
    "meta": {
      "type": ["object", "null"]
    },
    "data": {
      "type": "object",
      "required": ["id", "user_id", "title", "body"],
      "properties": {
        "id": { "type": "integer" },
        "user_id": { "type": "integer" },
        "title": { "type": "string" },
        "body": { "type": "string" }
      }
    }
  }
}
